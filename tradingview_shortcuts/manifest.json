{
    "name": "tradingview_shortcuts",
    "description": "Add tradingview shortcuts!",
    "version": "1.0",
    "action": {},
    "manifest_version": 2,
    "content_scripts": [
        {
            "run_at": "document_idle",
            "matches": [
                "https://*.tradingview.com/chart/*"
            ],
            "js": [
                "injecter.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "shortcuts_script.js"
    ]
}